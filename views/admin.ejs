            </ul>
          </div>
        </div>

        <div class="col-4">
          <h3>CRUD</h3>
          <div class="btn-group-vertical">
            <button class="updategallery" type="button">UPDATE IMAGE GALLERY</button>
            <button class="updatevideos" type="button">UPDATE VIDEOS</button>
            <button class="updateshop" type="button">UPDATE SHOP ITEMS</button>
            <button class="updateworks" type="button">UPDATE FUTURE WORKS</button>
          </div>
        </div>
      </div>
      <div class="getdata container">
        <button id="backbtn">Go back</button>
        <!-- open gallery image form-->
        <a href="#addform" class="glightboxform">
          <button id="addbtn">Add</button>
        </a>
        <!-- open videos slider form-->
        <a href="#addformVideo" class="glightboxform2">
          <button id="addbtn2">Add</button>
        </a>
        <!-- open shop items form-->
        <a href="#addformShop" class="glightboxform3">
          <button id="addbtn3">Add</button>
        </a>
        <!-- open future works form-->
        <a href="#addformWorks" class="glightboxform4">
          <button id="addbtn4">Add</button>
        </a>

        <div class="table-responsive-sm" id="table-wrapper" style="overflow-y: scroll; height:500px;">
          <table class="table table-dark table-hover table-striped" id="imagesTable">
            <thead>
              <caption>Images</caption>
              <tr>
                <th scope="col" style="display: none;">#</th>
                <th scope="col">Thumbnail</th>
                <th scope="col">Title</th>
                <th scope="col">Description</th>
                <th scope="col">Delete</th>
              </tr>
            </thead>
            <tbody>
              <% imagesDB.forEach(function(item) { %>
                <tr>
                  <td style="display: none;">
                    <%= item.id %>
                  </td>
                  <td><img src="<%= item.href %>" style="height: 25px; width:auto;"></td>
                  <td>
                    <%= item.title %>
                  </td>
                  <td>
                    <%= item.desc %>
                  </td>
                  <td><i class="bi bi-trash"></i></td>
                </tr>
                <% }) %>
            </tbody>
          </table>

          <table class="table table-dark table-hover table-striped" id="videosTable">
            <thead>
              <caption>Videos</caption>
              <tr>
                <th scope="col" style="display: none;">#</th>
                <th scope="col">Thumbnail</th>
                <th scope="col">Title</th>
                <th scope="col">Delete</th>
              </tr>
            </thead>
            <tbody>
              <% videosDB.forEach(function(item) { %>
                <tr>
                  <td style="display: none;">
                    <%= item.id %>
                  </td>
                  <td><img src="<%= item.thumb %>" style="height: 25px; width:auto;"></td>
                  <td>
                    <%= item.title %>
                  </td>
                  <td><i class="bi bi-trash"></i></td>
                </tr>
                <% }) %>
            </tbody>
          </table>

          <table class="table table-dark table-hover table-striped" id="itemsTable">
            <thead>
              <caption>Shop items</caption>
              <tr>
                <th scope="col" style="display: none;">#</th>
                <th scope="col">Thumbnail</th>
                <th scope="col">Name</th>
                <th scope="col">Price</th>
                <th scope="col">Delete</th>
              </tr>
            </thead>
            <tbody>
              <% itemsDB.forEach(function(item) { %>
                <tr>
                  <td style="display: none;">
                    <%= item.id %>
                  </td>
                  <td><img src="<%= item.thumb %>" style="height: 25px; width:auto;"></td>
                  <td>
                    <%=item.name %>
                  </td>
                  <td>
                    <%=item.price%>
                  </td>
                  <td><i class="bi bi-trash"></i></td>
                </tr>
                <% }) %>
            </tbody>
          </table>

          <table class="table table-dark table-hover table-striped" id="worksTable">
            <thead>
              <caption>Future works</caption>
              <tr>
                <th scope="col" style="display: none;">#</th>
                <th scope="col">Thumbnail</th>
                <th scope="col">Delete</th>
              </tr>
            </thead>
            <tbody>
              <% worksDB.forEach(function(item) { %>
                <tr>
                  <td style="display: none;">
                    <%= item.id %>
                  </td>
                  <td><img src="<%= item.thumb %>" style="height: 25px; width:auto;"></td>
                  <td><i class="bi bi-trash"></i></td>
                </tr>
                <% }) %>
            </tbody>
          </table>
        </div>
        <button id="savebtn">Save changes</button>

        <div id="addform">
          <form>
            <div class="form-row">
              <div class="form-group col-md-6">
                <label for="imgtitle">Title</label>
                <input type="text" class="form-control" id="imgtitle" placeholder="Title" required>
              </div>
              <div class="form-group col-md-6">
                <label for="imgdesc">Description</label>
                <input type="text" class="form-control" id="imgdesc" placeholder="Description" required>
              </div>
              <div class="form-group">
                <i class="bi bi-upload"></i>
                <input accept="image/*" type="file" class="form-control-file" id="imgupload" required>
                <div id="uploadedimg"></div>
              </div>
              <div class="formbtns">
                <button class="cancel" type="button">Cancel</button>
                <button class="add" type="submit">Add</button>
              </div>
            </div>
          </form>
        </div>

        <div id="addformVideo">
          <form>
            <div class="form-row">
              <div class="form-group col-md-6">
                <label for="videotitle">Title</label>
                <input type="text" class="form-control" id="videotitle" placeholder="Title" required>
              </div>
              <div class="form-group">
                <i class="bi bi-upload">video</i>
                <input accept="video/*" type="file" class="form-control-file" id="videoupload" required>
              </div>
              <div class="form-group">
                <i class="bi bi-upload">thumbnail</i>
                <input accept="image/*" type="file" class="form-control-file" id="videothumbupload" required>
                <div id="uploadedimgvideo"></div>
              </div>
              <div class="formbtns">
                <button class="cancel" type="button">Cancel</button>
                <button class="add" type="submit">Add</button>
              </div>
            </div>
          </form>
        </div>

        <div id="addformShop">
          <form>
            <div class="form-row">
              <div class="form-group col-md-6">
                <label for="itemname">Name</label>
                <input type="text" class="form-control" id="itemname" placeholder="Name" required>
              </div>
              <div class="form-group col-md-6">
                <label for="itemprice">Price</label>
                <input type="number" class="form-control" id="itemprice" placeholder="Price" required>
              </div>
              <div class="form-group">
                <i class="bi bi-upload">File</i>
                <input type="file" class="form-control-file" id="itemupload" required>
              </div>
              <div class="form-group">
                <i class="bi bi-upload">Preview</i>
                <input type="file" class="form-control-file" id="itempreviewupload" required>
              </div>
              <div class="form-group">
                <i class="bi bi-upload">Thumbnail</i>
                <input accept="image/*" type="file" class="form-control-file" id="itemthumbupload" required>
                <div id="uploadedimgitem"></div>
              </div>
              <div class="formbtns">
                <button class="cancel" type="button">Cancel</button>
                <button class="add" type="submit">Add</button>
              </div>
            </div>
          </form>
        </div>

        <div id="addformWorks">
          <form>
            <div class="form-row">
              <div class="form-group">
                <i class="bi bi-upload">file</i>
                <input type="file" class="form-control-file" id="worksfileupload" required>
              </div>
              <div class="form-group">
                <i class="bi bi-upload">thumbnail</i>
                <input accept="image/*" type="file" class="form-control-file" id="worksthumbupload" required>
                <div id="uploadedimgworks"></div>
              </div>
              <div class="formbtns">
                <button class="cancel" type="button">Cancel</button>
                <button class="add" type="submit">Add</button>
              </div>
            </div>
          </form>
        </div>
      </div>
      <!--section 7 end-->
    </div>
  </div>
  <!--fullpage end-->
  <div id="footer">
    <i class="bi bi-caret-down-fill"></i>
    <p>SCROLL DOWN</p>
  </div>
</body>

</html>